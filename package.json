{
  "name": "sh-mobile-sdk",
  "version": "1.0.0",
  "description": "sh-mobile-sdk SimpleHold mobile SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && tsc --skipLibCheck",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimpleHold/sh-mobile-sdk.git"
  },
  "keywords": [
    "react-native",
    "simplehold",
    "ethereum",
    "dapp"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimpleHold/sh-mobile-sdk/issues"
  },
  "homepage": "https://github.com/SimpleHold/sh-mobile-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/url-parse": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "url-parse": "^1.4.0"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example"
    ]
  },
  "files": [
    "dist"
  ]
}
